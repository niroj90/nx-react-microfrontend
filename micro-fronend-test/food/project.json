{
  "name": "food",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "food/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/food",
        "index": "food/src/index.html",
        "baseHref": "/",
        "main": "food/src/main.ts",
        "tsConfig": "food/tsconfig.app.json",
        "assets": ["food/src/favicon.ico", "food/src/assets"],
        "styles": ["food/src/styles.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "food/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "food/src/environments/environment.ts",
              "with": "food/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "food/webpack.config.prod.js"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "food:build",
        "hmr": true,
        "port": 4204
      },
      "configurations": {
        "development": {
          "buildTarget": "food:build:development"
        },
        "production": {
          "buildTarget": "food:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["food/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "food:build",
        "watch": false,
        "port": 4204
      },
      "configurations": {
        "development": {
          "buildTarget": "food:build:development"
        },
        "production": {
          "buildTarget": "food:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "food/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
